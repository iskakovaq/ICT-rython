   "source": [
    "from math import tan, pi\n",
    "s = float(input(\"Enter the length: \"))\n",
    "n = int(input(\"Enter the number of sides: \"))\n",
    "area = (n*s**2) / (4*tan(pi/n))\n",
    "print(\"The area of the polygon is\", area)"
